# Dokploy Configuration

# MCP Server Dokploy Configuration
app_name = "mcp-server"
port = 8080
domain = "your-domain.com"
ssl_enabled = true
auto_deploy = true

# Health check configuration
health_check_path = "/health"
health_check_interval = 30
health_check_timeout = 5

# Resource limits
memory_limit = "512MB"
cpu_limit = "0.5"

# Environment
environment = "production"
